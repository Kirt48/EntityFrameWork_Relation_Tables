@model examen.Models.Envrioment

@{
    ViewData["Title"] = "Detalles";
    ViewData["Module"] = "Ambiente";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>@ViewData["Module"]</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Code)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Floor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Floor)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Instructor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Instructor.Document)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Torre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Torre.Zone)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.EnvriomentId">Editar</a> |
    <a asp-action="Index">Regresar</a>
</div>
